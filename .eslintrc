{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "react",
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", 2],
    "typedef": [
      "error",
      "arrow-call-signature",
      "arrow-parameter",
      "call-signature",
      "property-declaration"
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
